{
    "name":"Dark - WhatsApp ",
    "description":"Changes WhatsApp Theme To Dark Mde",
    "manifest_version" : 2,
    "version" : "0.3",
    "content_scripts" :[{
            "matches": ["<all_urls>"],
            "include_globs": ["*://*web.whatsapp.com/*"],
            "js" : ["script.js"]
        }],
    "permissions" : ["activeTab","*://web.whatsapp.com/*","webNavigation"]
}
